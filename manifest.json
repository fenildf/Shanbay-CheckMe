{
  "manifest_version": 2,
  "name": "Shanbay-CheckMe",
  "version": "0.0.2",
  "description": "扇贝查卡工具",
  "author": "VoldikSS",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Shanbay-CheckMe"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.shanbay.com/team/manage/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "activeTab",
    "https://www.shanbay.com/team/manage/*"
  ]
}
